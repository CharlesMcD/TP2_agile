@{
    ViewData["Title"] = "Home Page";
}

@model IEnumerable<string>

<h2>@ViewData["Name"].</h2>


<div class="row">
    <div class="col-md-3">
        <h2>Liste de noms de chien</h2>
        <ul>
            @foreach (var name in Model)
            {
                <li>@name</li>
            }
        </ul>
    </div>
</div>
